{
  "name": "my-flickr-app",
  "version": "1.0.0",
  "description": "my-flickr-app is an application that allows the user to search for a Flickr photos by tag name. The page loads 20 images initially and uses infinite scroller.",
  "main": "index.js",
  "scripts": {
    "clean:dist": "rm -f ./dist/*",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --config webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --mode production --progress  --config webpack.prod.js",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "<rootDir>/__tests__/config/index.js"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!src/index.js"
    ],
    "testURL": "http://localhost",
    "testRegex": ".*\\.spec\\.js$",
    "setupTestFrameworkScriptFile": "./__tests__/config/index.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/config/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__tests__/config/styleMock.js"
    },
    "globals": {
      "DEVELOPMENT": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibechev/my-flickr-app.git"
  },
  "author": "Ivan Bechev",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.1.1",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "check-prop-types": "^1.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-watch": "^5.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^16.8.6",
    "redhooks": "^1.1.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "bugs": {
    "url": "https://github.com/ibechev/my-flickr-app/issues"
  },
  "homepage": "https://github.com/ibechev/my-flickr-app#readme"
}
